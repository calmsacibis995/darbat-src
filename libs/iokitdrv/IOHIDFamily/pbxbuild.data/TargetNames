if [ "${Target}" = "IOHIDFamily" ]; then
	Targets='InstallHeaders
IOHIDSystem
IOHIDLib
IOHIDFamily'
elif [ "${Target}" = "IOHIDSystem" ]; then
	Targets='IOHIDSystem'
elif [ "${Target}" = "IOHIDLib" ]; then
	Targets='InstallHeaders
IOHIDLib'
elif [ "${Target}" = "InstallHeaders" ]; then
	Targets='InstallHeaders'
else
	Targets='InstallHeaders
IOHIDSystem
IOHIDLib
IOHIDFamily'
fi
